@model SlideItem
@{
    ViewData["Title"] = "Detail";
}

<div class="col-6">
    <h1>Detail</h1>
    <h2>@Model.Offer</h2>
    <p>@Model.Title</p>
    <p>@Model.Description</p>
    <img src="~/assets/images/shipping/icon/@Model.Photo" height="50px" />
    <a class="btn btn-info" asp-action="Index">Go Back</a>
</div>




